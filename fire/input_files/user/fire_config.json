                    "machine": "machine_name",
                    "sector": "get_machine_sector",
                    "louvre_label": "get_tile_louvre_label",
                    "louvre_index": "get_tile_louvre_index",
                    "location_labels": "location_labels",
                    "format_coord": "format_coord",
